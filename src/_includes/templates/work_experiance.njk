<div class="workesperience_section--content">
    {% for workesperience in data.workesperience %}
        <ul class="work_experiance">
        
        <h4><a target="_blank" href="{{workesperience.link}}">{{workesperience.company}}</a></h4>
        <div class="work_experiance--flex">
        <li>{{workesperience.role}}</li>
        <li><i class="fa fa-calendar-alt"></i>{{workesperience.start_date}}</li>
        </div>
        <li><i class="fa fa-map-marker"></i>{{workesperience.address}}</li>
        <li>{{workesperience.description}}</li>
        <li><i class="fa fa-map-marker"></i>{{workesperience.tools}}</li>
        </ul>
    {%endfor %}
    </div>