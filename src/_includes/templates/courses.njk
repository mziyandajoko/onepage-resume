<div>
  {% for course in data.courses %}
  <div class="courses">
      <p><strong>{{course.courseName}}</strong></p>
      <p><i class="fa fa-calendar-alt"></i>{{course.start_date}}</p>
      <p><i class="fa fa-graduation-cap"></i>{{course.skills}}</p>
    <p>{{course.discription}}</p>
    <p><a target="_blank" href="{{course.link}}"><i class="fa fa-link"></i>{{course.company}}</a></p>
</div>
  {% endfor %} 
</div>