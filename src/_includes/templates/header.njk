<div class="header_content">
{% for details in data.details %}
    <ul class="personal_details">
        <li><i class="fa fa-map-marker"></i>{{details.address}}</li>
        <li><a target="_blank" href="mailto:{{details.email}}"><i class="fa fa-at"></i>{{details.email}}</a></li>
        <li><a target="_blank" href="tel:{{details.phone}}"><i class="fa fa-phone"></i>{{details.phone}}</a></li>
        <li><a target="_blank" href="{{details.linkedIn}}"><i class="fa fa-linkedin"></i>LinkedIn</a></a></li>
       <li><a target="_blank" href="{{details.Github}}"><i class="fa fa-github"></i>Github</a></li>
        {# <li><a target="_blank" href="{{details.website}}"><i class="fa fa-external-link" aria-hidden="true"></i>Personal Website</a></p> #}
        </ul>     
    {%endfor %}
</div>